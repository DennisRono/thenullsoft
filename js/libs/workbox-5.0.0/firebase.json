{
  "functions": {
    "source": "demos/functions"
  },
  "hosting": {
    "public": "demos/public",
    "ignore": [
      "**/.*",
      "**/node_modules/**",
      "*.sh",
      "./*"
    ],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/", "function": "app"
      }, {
        "source": "/demo/*", "function": "app"
      }, {
        "source": "/demo/*/sw*.js", "function": "app"
      }, {
        "source": "/api/*", "function": "app"
      }
    ]
  }
}
